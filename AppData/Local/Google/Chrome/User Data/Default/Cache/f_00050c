<!-- 现档案存放单位 弹出对话框 自己填写的input text--> 
<div id="modal-table" class="modal fade" tabindex="-1" data-backdrop="static">
	<div class="modal-dialog" style="margin-top: 25%;">
		<form id="editForm" enctype="multipart/form-data" >
		   <div class="widget-box" style="background:white;">
			   <div class="widget-main form-horizontal">
					<label class="control-label no-padding-right" for="tFromName">现档案存放单位</label>
					<input type="text" id="tFromName" autocomplete="off" value="" placeholder="存放单位" style="width: 70%;"/>
					<button id="submitButton" type="button"><i class="fa fa-check" aria-hidden="true"></i></button>
					<button data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
				</div>
		   </div>
		</form>
