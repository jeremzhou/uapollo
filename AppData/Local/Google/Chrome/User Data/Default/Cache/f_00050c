<script type="text/javascript" language="javascript">
var scripts = [ null,"/static/assets/js/x-editable/bootstrap-editable.js",
                "/static/assets/plugins/layer/layer.js",
                 "/static/index/js/global.js",
        		"/static/assets/js/personSelectInfo.js",null ]
$('.page-content-area').ace_ajax('loadScripts', scripts, function() {
	jQuery(function($) {
		
		//select option 相同值删除 ,name 为select name 值 ,value 为option 的值 
		function comptoOption(name,value){
			$("select[name="+name+"] option").each(function(){
			   if($(this).val() == value){
				   $(this).remove();
			    }
		    });
		}
		//页面数据初始化
		$.ajax({
				type : "post",
				dataType : "json",
				url : "/person/personInfoPageInit",
				complete: function(xmlRequest) {
                      var resultinfo = eval("(" + xmlRequest.responseText + ")");
                      var data = resultinfo.result;
                      //hidden 赋予初始值
                      $("#state").val(data.state);
                      $("#gid").val(data.gid);
                      $("#sex").val(data.et_sex);
                      $("#nation").val(data.et_nation);
                      $("#nativetype").val(data.et_nativetype);
                      $("#diploma").val(data.et_diploma);
                      $("#degree").val(data.et_degree);
                      $("#politics").val(data.et_politics);
                      $("#skill level").val(data.et_skilllevel);
                      $("#metier").val(data.et_metier);
                      $("#hand").val(data.et_hand);
                      $("#marry").val(data.et_marry);
                      $("#procreate").val(data.et_procreate);
                      $("#figure").val(data.et_figure);
                      $("#tFromX").val(data.et_archivespalce);
                      $("#datgfs").val(data.et_datgfs);
                      $("#archivesmove").val(data.et_archivesmove);
                      $("#insztype").val(data.et_insztype);
                      $("#sex1").val(data.et_sex);
                      $("#nation1").val(data.et_nation);
                      $("#nativetype1").val(data.et_nativetype);
                      $("#diploma1").val(data.et_diploma);
                      $("#degree1").val(data.et_degree);
                      $("#politics1").val(data.et_politics);
                      $("#skilllevel1").val(data.et_skilllevel);
                      $("#metier1").val(data.et_metier);
                      $("#hand1").val(data.et_hand);
                      $("#marry1").val(data.et_marry);
                      $("#procreate1").val(data.et_procreate);
                      $("#figure1").val(data.et_figure);
                      $("#tFromX1").val(data.et_archivespalce);
                      $("#datgfs1").val(data.et_datgfs);
                      $("#archivesmove1").val(data.et_archivesmove);
                      $("#insztype1").val(data.et_insztype);
                      //页面显示的数据
                 
                      $("#et_name").val(data.et_name);
                      if(data.et_sex !=null){
                       comptoOption("et_sex",data.et_sex);
                       $("#et_sex1").append("<option value=\"" + data.et_sex+ "\" selected = 'selected' >" + data.et_sex + "</option>"); 
                      }
                      if(data.et_degree !=null){
                       comptoOption("et_degree",data.et_degree);
                       $("#et_degree1").append("<option value=\"" + data.et_degree+ "\" selected = 'selected' >" + data.et_degree + "</option>");//添加option 到最后一个位置 
                      }
                      $("#et_idcard").val(data.et_idcard);
                      $("#et_nation1").val(data.et_nation);
                      
                      $("#et_mobile1").val(data.et_mobile);
                      $("#et_email1").val(data.et_email);
                      $("#et_nativeplace").val(data.et_nativeplace);
                      if(data.et_nativetype !=null){
                      comptoOption("et_nativetype",data.et_nativetype);
                      $("#et_nativetype1").append("<option value=\"" + data.et_nativetype+ "\" selected = 'selected' >" + data.et_nativetype + "</option>");//添加option 到最后一个位置 
                      }
                      $("#et_ComputerID").val(data.et_ComputerID);
                      $("#et_perAcc1").val(data.et_perAcc);
                      if(data.et_hand !=null){
                      comptoOption("et_hand",data.et_hand);
                      $("#et_hand1").append("<option value=\"" + data.et_hand+ "\" selected = 'selected' >" + data.et_hand + "</option>");//添加option 到最后一个位置 
                      }
                      if(data.et_metier !=null){
                      comptoOption("et_metier",data.et_metier);
                      $("#et_metier1").append("<option value=\"" + data.et_metier+ "\" selected = 'selected' >" + data.et_metier + "</option>");//添加option 到最后一个位置  
                      }
                      if(data.et_diploma !=null){
                      comptoOption("et_diploma",data.et_diploma);
                      $("#et_diploma1").append("<option value=\"" + data.et_diploma+ "\" selected = 'selected' >" + data.et_diploma + "</option>");//添加option 到最后一个位置  
                      }
                      if(data.et_marry !=null){
                      comptoOption("et_marry",data.et_marry);
                      $("#et_marry1").append("<option value=\"" + data.et_marry+ "\" selected = 'selected' >" + data.et_marry + "</option>");//添加option 到最后一个位置  
                      }
                      $("#et_wifename").val(data.et_wifename);
                      $("#et_wifeidcard1").val(data.et_wifeidcard);
                      if(data.et_archivespalce !=null){
                      comptoOption("tFrom",data.et_archivespalce);
                      $("#tFrom1").append("<option value=\"" + data.et_archivespalce+ "\" selected = 'selected' >" + data.et_archivespalce + "</option>");//添加option 到最后一个位置
                      }
                      if(data.et_archivesmove !=null){
                      comptoOption("et_archivesmove",data.et_archivesmove);
                      $("#et_archivesmove1").append("<option value=\"" + data.et_archivesmove+ "\" selected = 'selected' >" + data.et_archivesmove + "</option>");//添加option 到最后一个位置
                      }
               }
		});
		
	   //为现档案存放单位 Select添加事件，当选择其中一项时触发 
       $("#tFrom1").change(function(){
    	   var checkText=$("#tFrom1").find("option:selected").val(); //获取Select选择的text 
    	   if(checkText=="0"){
    		   $("#modal-table").modal("toggle");
               $("#editForm")[0].reset();
               $("#modal-tip").html("");
    	   }else if(checkText=="深圳市人才交流服务中心总部"||checkText=="深圳市人才交流服务中心高新分部"||checkText=="深圳市职业介绍服务中心"||checkText=="深圳市福田人力资源中心"||checkText==""){
    		   return;
    	   }else{
    		   comptoOption("tFrom",checkText);//清除之前手工填写的option
    	   }  
